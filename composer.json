{
    "name": "daniel-km/omeka-s-module-adminer",
    "type": "library",
    "description": "Access to Omeka database through Adminer, a one-file full-featured database management tool.",
    "keywords": [
        "Omeka-S",
        "module",
        "admin",
        "curation",
        "adminer",
        "mysql"
    ],
    "license": "CECILL-2.1",
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-Adminer",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.git@Berthereau.net"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-Adminer/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "require": {
        "sempia/external-assets": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Adminer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminerTest\\": "tests/AdminerTest/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "sempia/external-assets": true
        }
    },
    "extra": {
        "installer-name": "Adminer",
        "external-assets": {
            "asset/vendor/": "https://github.com/Daniel-KM/Omeka-S-module-Adminer/releases/download/3.4.9-5.4.1/adminer-compiled-5.4.1.tar.gz"
        }
    }
}
